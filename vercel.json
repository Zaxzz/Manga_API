{
  "version": 2,
  "builds": [
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/manga(.*)",
      "dest": "server/app.js"
    },
    {
      "src": "/api/mangaList(.*)",
      "dest": "server/app.js"
    },
    {
      "src": "/api/search(.*)",
      "dest": "server/app.js"
    },
    {
      "src": "/status",
      "dest": "server/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "server/app.js"
    }
  ]
}
